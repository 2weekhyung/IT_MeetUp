<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <!-- mybatis 환경 설정 파일 활용 -->
 <mapper namespace="com.itmeetup.mybatis.AssignmentDAO">
 
 	<select id="" resultType="com.itmeetup.mybatis.AssignmentDTO">
 		SELECT "CODE", "START", "END", "PROFILE", "NICKNAME", "TITLE", "CONTENT", "VIEW", "DAY"
		FROM POPULAR_VIEW
 	</select>
 	<select id="newList" resultType="com.itmeetup.mybatis.AssignmentDTO">
 		SELECT "CODE", "START", "END", "PROFILE", "NICKNAME", "TITLE", "CONTENT", "VIEW", "DAY"
		FROM NEW_VIEW
 	</select>

 </mapper>