<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="com.itmeetup.mybatis.IJobDAO">

	
	<!-- 프로젝트 정보 출력 -->
	<select id="jobList" resultType="java.lang.String" >
	
		SELECT J.JOB_NAME AS "JOB"
		FROM TEAM_JOB TE_J
		JOIN JOB J  ON J.JOB_CODE = TE_J.TJ_JOB_CODE
		WHERE TJ_OP_CODE = #{code}
		
	</select>
</mapper>













