<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="com.itmeetup.mybatis.IJobDAO">

	
	<!-- 해당 프로젝트 희망 직위 출력 -->
	
	<select id="jobList" resultType="java.lang.String" >
	
		SELECT J.JOB_NAME "JOB"
		FROM TEAM_JOB TE_J
		JOIN JOB J  ON J.JOB_CODE = TE_J.TJ_JOB_CODE
		WHERE TJ_OP_CODE = #{code}
		
	</select>
	
	
	<!-- 직위 출력  -->
	<select id="jobs" resultType="com.itmeetup.mybatis.JobDTO" >
	SELECT JOB_CODE "JOBCODE" , JOB_NAME "JOBNAME" 
	FROM JOB
	</select>
</mapper>













